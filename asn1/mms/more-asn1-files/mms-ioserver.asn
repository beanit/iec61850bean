	},
 
	-- Simple Type -------- Size ---------
	boolean		[3] IMPLICIT NULL,
	bit_string		[4] IMPLICIT Integer32,
	integer		[5] IMPLICIT Unsigned8,
	unsigned		[6] IMPLICIT Unsigned8,
